{
  "name": "auth-me-project",
  "version": "1.0.0",
  "description": "Repo for Auth Me project # Park King",
  "main": "index.js",
"scripts": {

  "install": "npm --prefix backend install backend",
  "sequelize": "npm run --prefix backend sequelize",
  "start": "npm start --prefix backend",
  "build": "npm run --prefix backend build",
  "dev:backend": "npm --prefix backend run dev",
  "dev:frontend": "npm --prefix frontend run dev",
  "dev": "npm run dev:backend & npm run dev:frontend",
  "db:migrate": "cd backend && npx sequelize-cli db:migrate",
  "db:seed:all": "cd backend && npx sequelize-cli db:seed:all",
  "db:seed:undo:all": "cd backend && npx sequelize-cli db:seed:undo:all"
},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joe-corcoran/park-king.git"
  },
